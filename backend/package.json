{
  "name": "hk01-test",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r babel-register\"",
    "clean": "rm -rf dist/",
    "build": "npm run clean && babel src -s -d dist",
    "test": "jest --coverage --forceExit",
    "lint": "eslint src test --fix"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "card-validator": "^4.3.0",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^5.0.0",
    "express": "~4.15.5",
    "express-validator": "^5.0.1",
    "helmet": "^3.11.0",
    "lodash": "^4.17.5",
    "mongodb": "^3.0.3",
    "redis": "^2.8.0",
    "stripe": "^5.5.0",
    "winston": "^3.0.0-rc1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.18.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "flow-bin": "^0.66.0",
    "jest": "^22.4.2",
    "nodemon": "^1.15.1",
    "supertest": "^3.0.0",
    "uuid": "^3.2.1"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "**/src/**/*.js"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "license": "MIT",
  "description": "HK01 offsite test backend api server"
}
